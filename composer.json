{
	"name": "tarlepp/symfony-flex-backend",
	"description": "JSON REST API which is build on top of Symfony framework.",
	"version": "0.2.1",
	"type": "project",
	"keywords": [
		"Symfony",
		"Flex",
		"API"
	],
	"homepage": "https://github.com/tarlepp/symfony-flex-backend",
	"license": "MIT",
	"authors": [
		{
			"name": "Tarmo Lepp√§nen",
			"email": "tarmo.leppanen@protacon.com",
			"homepage": "https://github.com/tarlepp",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/tarlepp/symfony-flex-backend/issues"
	},
	"prefer-stable": true,
	"require": {
		"php": "^7.2.0",
		"ext-iconv": "*",
		"doctrine/annotations": "1.6.0",
		"doctrine/doctrine-fixtures-bundle": "3.0.2",
		"doctrine/doctrine-migrations-bundle": "1.3.1",
		"easycorp/easy-log-handler": "1.0.4",
		"lexik/jwt-authentication-bundle": "2.4.4",
		"matthiasnoback/symfony-console-form": "3.1.0",
		"nelmio/api-doc-bundle": "3.2.1",
		"nelmio/cors-bundle": "1.5.4",
		"piwik/device-detector": "3.10.2",
		"ramsey/uuid": "3.7.3",
		"sensio/framework-extra-bundle": "5.1.6",
		"stof/doctrine-extensions-bundle": "1.3.0",
		"symfony/asset": "4.0.9",
		"symfony/console": "4.0.9",
		"symfony/expression-language": "4.0.9",
		"symfony/flex": "1.0.80",
		"symfony/form": "4.0.9",
		"symfony/framework-bundle": "4.0.9",
		"symfony/lts": "^4@dev",
		"symfony/monolog-bundle": "3.2.0",
		"symfony/orm-pack": "1.0.5",
		"symfony/security-bundle": "4.0.9",
		"symfony/serializer": "4.0.9",
		"symfony/twig-bundle": "4.0.9",
		"symfony/validator": "4.0.9",
		"symfony/yaml": "4.0.9"
	},
	"require-dev": {
		"codedungeon/phpunit-result-printer": "0.18.7",
		"liuggio/fastest": "1.6.1",
		"php-coveralls/php-coveralls": "2.0.0",
		"phpmetrics/phpmetrics": "2.3.2",
		"phpstan/phpstan-shim": "0.9.2",
		"phpunit/phpcov": "5.0.0",
		"phpunit/phpunit": "7.1.5",
		"roave/security-advisories": "dev-master",
		"sebastian/finder-facade": "1.2.2",
		"sensiolabs/security-checker": "4.1.8",
		"squizlabs/php_codesniffer": "3.2.3",
		"sroze/companienv": "0.0.8",
		"symfony/browser-kit": "4.0.9",
		"symfony/debug-pack": "1.0.5",
		"symfony/dotenv": "4.0.9",
		"symfony/maker-bundle": "1.2.0",
		"symfony/phpunit-bridge": "4.0.9",
		"symfony/requirements-checker": "1.1.1",
		"symfony/web-server-bundle": "4.0.9",
		"symplify/easy-coding-standard": "4.2.3",
		"vimeo/psalm": "1.1.3"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear --no-warmup": "symfony-cmd",
			"cache:warmup": "symfony-cmd",
			"assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script",
			"security-checker security:check --end-point=http://security.sensiolabs.org/check_lock": "script"
		},
		"post-install-cmd": [
			"@auto-scripts",
			"Companienv\\Composer\\ScriptHandler::run"
		],
		"post-update-cmd": [
			"@auto-scripts",
			"Companienv\\Composer\\ScriptHandler::run"
		]
	},
	"conflict": {
		"symfony/symfony": "*",
		"symfony/twig-bundle": "<3.3",
		"symfony/debug": "<3.3"
	},
	"extra": {
		"projectTitle": "Symfony Flex Backend",
		"symfony": {
			"id": "01BFA135DXTPRA5CR250NTJJJP",
			"allow-contrib": "true"
		}
	}
}
